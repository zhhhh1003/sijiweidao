@import "../common/tab.wxss";
@import "../common/common.wxss";
@import "../myorder/myorder.wxss";
.goods-content{
  width:100%;
}
.orders-box{
  width:100%;
}
.orders-number{
    height: 70rpx;
    line-height: 70rpx;
    border-bottom: none;
}
.orders-money{
    height:70rpx;
    line-height: 70rpx;
   border-bottom: 1rpx solid #e9e9e9;
}
.orders-detail{
  border-bottom: none;
}
.orders-list{
  border-bottom: 6px solid #e9e9e9;
}
/*全部订单为空时*/

.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
} 
.orders-footer .receive-btn{
    float: right;
    padding:0rpx 30rpx;
    line-height:50rpx;
    border-radius: 6rpx;
    margin-left: 10rpx;
    color: #ff0033;
    background: none;
    border:1px solid #ff0033;
}